{
  "name": "server",
  "description": "Quizdini teach app",
  "config": {
    "comment": "Quizdini teacher site source code"
  },
  "version": "0.2.0",
  "private": true,
  "engines": {
    "node": "15.x",
    "npm": "7.x"
  },
  "scripts": {
    "start": "node server/bin/www",
    "server": "nodemon server/bin/www",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "prettier": "prettier **/*.{js,ts,vue,json} --print-width=100 --write",
    "deploy-to-prod": "npm run prettier && npm run build --prefix client && git add . && cross-env git commit --allow-empty-message --no-verify && git push origin main && git push heroku main",
    "heroku-prebuild": "echo Hook for Heroku prebuild...",
    "heroku-postbuild": "echo Hook for Heroku postbuild..."
  },
  "dependencies": {
    "aws-sdk": "^2.839.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.17.0",
    "date-fns-tz": "^1.1.1",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-sslify": "^1.2.0",
    "helmet": "^4.4.1",
    "http-status-codes": "^2.1.4",
    "md5": "^2.3.0",
    "memjs": "^1.3.0",
    "mongoose": "^5.11.15",
    "morgan": "~1.9.1",
    "nanoid": "^3.1.20",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "shortid": "^2.2.16",
    "stripe": "^8.134.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  }
}
